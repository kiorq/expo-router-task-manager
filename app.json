{
  "expo": {
    "name": "expo-router-taskmanager",
    "slug": "expo-router-taskmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      },
      "bundleIdentifier": "com.kiorq.exporoutertaskmanager"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.kiorq.exporoutertaskmanager"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c579930a-8286-40dd-b2ab-fd333af19589"
      }
    }
  }
}
